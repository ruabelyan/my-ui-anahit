{
  "name": "@my-ui/react",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "build": "yarn build:react && yarn build:types",
    "build:react": "react-scripts build",
    "build:types": "tsc -p tsconfig.gen-dts.json && tscpaths -p tsconfig.gen-dts.json -s ./src -o ./build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.5.0-alpha.39",
    "@storybook/addon-backgrounds": "^6.4.19",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.5.0-alpha.39",
    "@storybook/addon-interactions": "^6.5.0-alpha.39",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.0-alpha.39",
    "@storybook/addon-storysource": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/builder-webpack5": "^6.5.0-alpha.39",
    "@storybook/manager-webpack5": "^6.5.0-alpha.39",
    "@storybook/node-logger": "^6.5.0-alpha.39",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.5.0-alpha.39",
    "@storybook/testing-library": "^0.0.9",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.25",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-transition-group": "^4.4.5",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "react-scripts-customized": "github:bangits/react-scripts#1.8.0",
    "sass": "^1.49.7",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@my-ui/core": "^0.1.0",
    "@types/react-transition-group": "^4.4.5",
    "classnames": "^2.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-transition-group": "^4.4.2"
  }
}
